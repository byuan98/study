package Text;

import java.io.File;

public class FileNameTest {
    public static void main(String[] args) {
        File file=new File("D:\\Code\\Java\\study");
//        File file=new File("D:\\Code\\FileTest\\34作业");
        File[] fileAll=file.listFiles();
     
        for (File filePart : fileAll) {
            System.out.println(filePart.getName());
            String name=filePart.getName();
            System.out.println(name.substring(0,3));
//            File f=new File("D:\\Code\\FileTest\\34作业\\"+1+filePart.getName());
//            Boolean b= filePart.renameTo(f);
//            System.out.println(b);
        }
    }
}
