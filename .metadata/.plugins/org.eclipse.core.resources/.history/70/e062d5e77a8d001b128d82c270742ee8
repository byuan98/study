<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ page import="entity.Student"%>
<%@ taglib prefix="core" uri="http://java.sun.com/jsp/jstl/core"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>JSP之JSTL表达式</title>
</head>
<body>
  <!-- JSTL之核心标签库：core -->
    <!-- 4、remove -->
    <!-- 删除域中指定属性，等价于removeAttribute(xxx) -->
      <core:set scope="page" var="name" value="page_value"/>
      <core:set scope="request" var="name" value="request_value"/>
      <core:set scope="session" var="name" value="session_value"/>
      <core:set scope="application" var="name" value="application_value"/>

</body>
</html>